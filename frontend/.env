# Frontend Environment Variables - NEXUS-CORE
# DO NOT COMMIT THIS FILE TO VERSION CONTROL

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Environment
VITE_APP_ENV=development
VITE_APP_VERSION=2.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_SENTRY=false
VITE_ENABLE_DEBUG_MODE=true

# Application Configuration
VITE_APP_NAME="NEXUS-CORE"
VITE_APP_DESCRIPTION="NGX Performance & Longevity Control Center"
VITE_MAX_FILE_SIZE=10485760  # 10MB in bytes

# NOTE: Firebase variables removed as Firebase is no longer used
# This project uses Supabase for authentication and database operations